<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practiceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SampleIntent"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Basic">
        <activity android:name=".LifeCycleMenuActivity"></activity>
        <activity android:name=".ParcelableMenuActivity" />
        <activity
            android:name=".ForResultActivity"
            android:label="메뉴 액티비티"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".LifeCycleActivity">

            <!-- with Activity:: LandscapeActivity -->
            <!-- <activity android:name=".LandscapeActivity" -->
            <!-- android:configChanges="orientation|screenSize|keyboardHidden" -->
            <!-- android:screenOrientation="landscape"> -->

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>